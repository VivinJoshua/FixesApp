@model List<FixesApp.Models.WorkDetails>
@{
    ViewBag.Title = "My Works";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_WorkerNav")
<h2>Worker Main</h2>

@if (Model.Count == 0)
{
    <p>No works done yet </p>
}
else
{
    <table class="table">
        <tr>
            <th>Booking Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Mobile</th>
            <th>Completed Date</th>


        </tr>
        @foreach (var val in Model)
        {
            <tr>
                <td>@val.WorkDetailsId</td>
                <td>@val.UserName</td>
                <td>@val.User.Address</td>
                <td>@val.UserMobile</td>
                <td>@val.WorkdoneDT</td>
            </tr>
        }

    </table>

}
