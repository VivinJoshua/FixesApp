@model List<FixesApp.Models.WorkDetails>
@{
    ViewBag.Title = "Main";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_WorkerNav")
<h2>Worker Main</h2>
@if(Model.Count==0)
{
    <p>No Works Available</p>
}
else
{
<table class="table">
    <tr>
        <th>Booking Id</th>
        <th>Name</th>
        <th>Address</th>
        <th>Mobile</th>
        <th>Request Date</th>
        <th></th>
    </tr>
    @foreach (var val in Model)
    {
        <tr>
            <td>@val.WorkDetailsId</td>
            <td>@val.UserName</td>
            <td>@val.User.Address</td>
            <td>@val.UserMobile</td>
            <td>@val.RequestDT</td>
            <td><a href="@Url.Action("WorkCompleted","Worker", new { id = val.WorkDetailsId })" class="btn btn-success" ;">Completed</a></td>
        </tr>
    }

</table>
}

